<% title "Login" %>

<% form_for @user_session do |f| %>
  	<fieldset>
	<%= f.error_messages %>
	<ul>
		<li>
			<%= f.label :username %>
			<%= f.text_field :username, :class => "text" %>
		</li>
		<li>
			<%= f.label :password %>
			<%= f.password_field :password, :class => "text" %>
		</li>
		<li class="submit"><%= f.submit "Submit" %></li>
	</ul>
	</fieldset>
<% end %>


